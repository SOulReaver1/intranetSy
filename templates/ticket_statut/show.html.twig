{% extends 'base.html.twig' %}

{% block title %}TicketStatut{% endblock %}
{% set title = ticket_statut.name %}
{% block header %}
    <a href="{{ path('ticket_statut_index') }}" class="btn btn-primary">Retour</a>
{% endblock %}
{% block body %}

    <table class="table">
        <tbody>
            <tr>
                <th>#</th>
                <td>{{ ticket_statut.id }}</td>
            </tr>
            <tr>
                <th>Nom :</th>
                <td>{{ ticket_statut.name }}</td>
            </tr>
        </tbody>
    </table>

    <div class="col mt-3">
        <div class="row">
            <a href="{{ path('ticket_statut_edit', {'id': ticket_statut.id}) }}" class="btn btn-primary">Modifier</a>

            {{ include('ticket_statut/_delete_form.html.twig') }}
        </div>
    </div>
{% endblock %}
