{% extends 'base.html.twig' %}

{% block title %}HelpStatut{% endblock %}
{% set title = help_statut.name %}
{% block header %}
    <a href="{{ path('help_statut_index') }}" class="btn btn-info">Retour</a>
{% endblock %}
{% block body %}
    <table class="table">
        <tbody>
            <tr>
                <th>#</th>
                <td>{{ help_statut.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ help_statut.name }}</td>
            </tr>
        </tbody>
    </table>

    <div class="row mt-3">
        <a href="{{ path('help_statut_edit', {'id': help_statut.id}) }}" class="btn btn-info">Modifier</a>

        {{ include('help_statut/_delete_form.html.twig') }}
    </div>
    
{% endblock %}
