{% extends 'base.html.twig' %}

{% set title = help_statut.name %}
{% block header %}
    <a href="{{ path('help_statut_index') }}" class="btn btn-primary">Retour</a>
{% endblock %}
{% block body %}
    <table class="table">
        <tbody>
            <tr>
                <th>#</th>
                <td>{{ help_statut.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ help_statut.name }}</td>
            </tr>
        </tbody>
    </table>

    <div class="col mt-4">
        <div class="row">
            <a href="{{ path('help_statut_edit', {'id': help_statut.id}) }}" class="btn btn-primary">Modifier</a>
            {{ include('help_statut/_delete_form.html.twig') }}
        </div>
    </div>
    
{% endblock %}
